<UserControl x:Class="BEServerManager.View.Home.WebApi.WebApiMainUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BEServerManager.View.Home.WebApi"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid >
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200"/>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>
            <Border Background="Transparent" BorderBrush="#252526" BorderThickness="0,0,5,0"  Grid.Column="0">
                <local:WebApiControlMenuUserControl/>
            </Border>
            <local:WebApiLogUserControl  Grid.Column="1"/>
        </Grid>
    </Grid>
</UserControl>
